{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],

    "rewrites": [
      {
        "source": "/images/(.*)",
        "destination": "/images/$1"
      }
    ]
  }